<%
path=request.Form("path")
query=request.Form("query")
query=replace(query,"*","&")
beforepath=path&"?"&query
id=request.Form("id")
model_number=request.Form("model_number")
SQL="select * from PART_MODEL where ITEM_ID='"&id&"'"
rs.open SQL,conn,1,3
if not rs.eof then
lead_time=timeconvert(request.Form("lead_time"),request.Form("lead_time_unit"))
rs("LEAD_TIME")=lead_time
rs.update
rs.close
word="Successfully edit Part Model."
action="location.href='"&beforepath&"'"
else
word="Part Model of "&model_number&" has not existed, please input again."
action="history.back()"
end if
%>
